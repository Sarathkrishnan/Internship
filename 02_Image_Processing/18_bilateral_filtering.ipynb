   "source": [
    "import cv2\n",
    "  \n",
    "# Read the image.\n",
    "img = cv2.imread('apples.jpg')\n",
    "  \n",
    "# Apply bilateral filter with d = 15, \n",
    "# sigmaColor = sigmaSpace = 75.\n",
    "bilateral = cv2.bilateralFilter(img, 15, 75, 75)\n",
    "  \n",
    "# Save the output.\n",
    "cv2.imwrite('blured_apple.jpg', bilateral)"
   ]
  }
