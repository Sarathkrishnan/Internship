    "import numpy as np\n",
    "import cv2\n",
    "  \n",
    "# Open the image.\n",
    "img = cv2.imread('cat_damaged.png')\n",
    "  \n",
    "# Load the mask.\n",
    "mask = cv2.imread('cat_mask.png', 0)\n",
    "  \n",
    "# Inpaint.\n",
    "dst = cv2.inpaint(img, mask, 0, cv2.INPAINT_NS)\n",
    "  \n",
    "# Write the output.\n",
    "cv2.imwrite('cat_inpainted.png', dst)"
   ]
  }
